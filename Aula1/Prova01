numero1 = float(input("Digite o número inteiro e positivo 1: "))
numero2 = float(input("Digite o número inteiro e positivo 2: "))

soma = numero1 + numero2
multiplicacao = numero1 * numero2
media = soma / 2
quadrado = numero1 * numero1
cubo = numero2 * numero2 * numero2

print("A soma dos valores é: ", soma)
print("A multiplicação dos valores é: ", multiplicacao)
print("O quadrado do primeiro é: ", quadrado)
print("O cubo do segundo é: ", cubo)
